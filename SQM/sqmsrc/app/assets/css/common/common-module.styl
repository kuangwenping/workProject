//树
.bui-tree-list{
  background:mcb;
  overflow :auto;
  padidng-top:10px;
  padidng-bottom:10px;
  ul{
    overflow: hidden;
  }
  .bui-tree-item {
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
    cursor: pointer;
    padding:0 10px;
    line-height:25px;
  }
  //+ -号
  .bui-tree-item-expanded{
    .x-tree-icon-wraper{
      .x-tree-elbow-expander {
        background:mc;
        &:after {
          pseudo('-');
          font-size: 30px;
          margin-top: -6px;
        }
      }
    }
  }

  .bui-tree-item-checked{
    .x-tree-icon-wraper{
      .x-tree-icon-checkbox,.x-tree-icon-radio{
        background-position :0 -48px;
      }
    }
  }


  .x-tree-icon-wraper{
    .x-tree-elbow-expander{
      &:after {
        pseudo('+');
        font-size: 22px;
        font-family: bold;
        margin-top: -5px;
        margin-left: -1px;
        color:mcb;
      }
      background :ft2;
      width:14px;
      height:14px;
      line-height:14px;
      text-align center;
      display:inline-block;
      margin-right:6px;
      vertical-align: middle;
    }
    .x-tree-icon-checkbox,.x-tree-icon-radio{

      display:inline-block;
      width:23px;
      height:23px;
      font-weight:inherit;
      sbg('../../img/check.png',0,0);
      vertical-align middle;
    }
    .x-tree-elbow-dir,.x-tree-elbow-leaf{
      display:none;
    }
    .x-tree-elbow-line,.x-tree-elbow,.x-tree-elbow-end{
      display:inline-block;
      width:20px;
    }
  }


  .bui-tree-item-selected{
    .x-tree-icon-wraper{

    }
  }

}

