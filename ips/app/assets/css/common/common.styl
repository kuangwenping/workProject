  //-----------分页样式------------//
  .aside-pages.pages-thunk{
    position:absolute;
    bottom:10px;
    padding:0 8px;
    .bui-bar-item{
      setMar0();
      margin-right:1px;
      a{
        &:hover{
          background:$bg5-1;
        }
        border:none;
        background:$bg9;
        color:$fc3;
      }
    }
    #prev,#next{

      a{
        &:before{
          background:$bg9;
        }
      }
      a{
        &:after{
          border-color:$fc3;
        }
      }
    }
    #prev.disabled,#next.disabled{
      a{
        &:after{
          border-color:$mc;
        }
      }
    }
  }

  .pages-thunk.ui-pages-center{
    text-align center;
    ul.bui-pagingbar{
      display:inline-block;
      float:none;
    }
  }

  .pages-thunk{
    .bui-bar-item.active{
      a{
        background:$ac;
        color:$fc3;
        border:none;
      }
    }
    .bui-bar-item{
      width:32px;
      height:32px;
      text-align: center;
      line-height:30px;
      display:inline-block;
      a{
        display:inline-block;
        width:100%;
        height:100%;
        color:$mc;
        padding:0;
        border-color: $bm;
        background: $bgb;
        line-height:32px;
      }
    }

    .disabled#prev{
      a{
        &:after{
          border-bottom: 1px solid $bg1;
          border-left: 1px solid $bg1;
          transform: rotate(45deg);
        }
      }
    }
    .disabled#next{
      a{
        &:after{
          border-bottom: 1px solid $bg1;
          border-left: 1px solid $bg1;
          transform: rotate(-135deg);
        }
      }
    }

    #prev,#next{
      a{
        position: relative;
        &:hover{
          opacity: .9;
        }
        &:before{
           addWC('');
          position: absolute;
          width: 100%;
          height:100%;
          background:$bgb;
          top:0;
          left:0;
        }
        &:after{
           addWC('');
          position: absolute;
          top:10px;
          left:12px;
          width:10px;
          height:10px;
        }
      }
    }
    #prev{
      a{
        &:after{
          border-bottom: 1px solid $ac;
          border-left: 1px solid $ac;
          transform: rotate(45deg);
        }
      }
    }
    #next{
      a{
        &:after{
          border-bottom: 1px solid $ac;
          border-left: 1px solid $ac;
          transform: rotate(-135deg);
        }
      }
    }
  }


