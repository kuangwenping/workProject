  input.redbd{
    border-color:$ac1;
  }

  input.greenbd{
    border-color:$ac3;
  }
  //绿色背景
  .greenbg{
    background-color:#C4EAC4 !important;
  }
  //粉色背景
  .pinkbg{
    background-color:#ffd3d3 !important;
  }

  input[type="text"].greenbg{
    background-color:#C4EAC4 !important;
  }

  input[type="text"].pinkbg{
    background-color:#ffd3d3 !important;
  }


  input[type="checkbox"]{
    display:none;
  }
  .ui-check-label{
    position:relative;
    user-select: none;
    &:before{
      addWC(' ');
      width:18px;
      height:18px;
      display:inline-block;
      margin-right:5px;
      border:1px solid $bm;
      background:$bgb;
      vertical-align: middle;
      cursor pointer;
    }
  }
  input[type="checkbox"]:checked + .ui-check-label{
    &:after{
      position:absolute;
      left:1px;
      top:3px;
      addWC(' ');
      height:10px;
      width:5px;
      border-bottom: 1px solid $ac;
      border-right: 1px solid $ac;
      transform: rotate(45deg);
      margin-left:6px;
      margin-bottom:7px;
    }
  }


  .ui-check-label.check{
    &:after{
      position:absolute;
      left:1px;
      top:3px;
      addWC(' ');
      height:10px;
      width:5px;
      border-bottom: 1px solid $ac;
      border-right: 1px solid $ac;
      transform: rotate(45deg);
      margin-left:6px;
      margin-bottom:7px;
    }
  }

  input[readonly="readonly"]{
    background:$bg1 !important;
  }

  input[readonly="readonly"]+.ui-check-label{
    background:$bg1 !important;
  }

  input[readonly="readonly"]:checked +.ui-check-label{
    &:after{
      addWC('');
      border:none;
    }
  }

  input[disabled="disabled"]{
    background:$bg3 !important;
  }

  textarea[readonly="readonly"]{
    background:$bg1;
  }

  textarea[disabled="disabled"]{
    background:$bg3 !important;
  }

  .disabled-bg {
    background :$bg3;
  }


  //表单
  .ui-form{
    label{
      margin-right:5px;
    }
  }

  //textarea
  .ui-textarea{
    resize:none;
    border:1px solid $bm;
    background:$bgb;
    font-size:12;
    padding:10px 20px;
  }

  //修改记录
  .amend-record{
    padding-top:10px;
    border-bottom:1px solid $mc;
    padding-bottom:5px;

    .more-btn{
      cursor pointer;
    }
  }
  //----------------------bui修改暴力拆改线------------------------//

  //操作表格
  .grid-bar-thunk{
    .bui-grid-button-bar{
      float:right;
    }
  }

  //表格
  .table-thunk-check{
  //操作头列
    .bui-grid-table tr td:first-child, .bui-grid-table tr th:first-child{
      width:80px !important;
    }
  }
  .table-thunk {
    a{
      text-decoration:underline;
      cursor:pointer;
    }

    //尾列
    .bui-grid-border .bui-grid-cell-empty, .bui-grid-border .bui-grid-hd-empty{
      display:none;
    }

    //-----cheeck-----//
      //check框
    .bui-grid .x-grid-checkbox{
      display:inline-block;
      width:18px;
      height:18px;
      border:1px solid $bm;
      background:$bgb;
      vertical-align: middle;
    }
        //选中
    .bui-grid-row-selected,.checked{
      .x-grid-checkbox{
        border-color:$ac;
        &:after{
          addWC(' ');
          width:5px;
          height:10px;
          border-bottom: 1px solid $ac;
          border-right: 1px solid $ac;
          transform: rotate(45deg);
          margin-bottom:18px;
        }
      }
    }
      //-----选中----//
    .bui-grid .bui-grid-table{
      .bui-grid-row-hover,.bui-grid-hd-hover{
        background:$bg6;
        .x-grid-checkbox{
          border:1px solid $ac;
        }
      }
    }
    .bui-grid-header-hover .bui-grid-table{
      .bui-grid-hd-hover{
        .bui-grid-hd-inner{
          background:$bg6 ;
        }
      }
    }
    .bui-simple-grid .bui-grid-table .bui-grid-row-selected, .bui-grid .bui-grid-table .bui-grid-row-selected{
      background:$bg6 ;
    }

      //-----单元格-----//
    .bui-grid-table{
      .red,.green,.yellow{
        .bui-grid-cell{
          .uploadifive-button{
            color:$fc3;
            }
        }  
      }
      .bui-grid-hd{
          border-color:$bm;
          border-bottom: none;
        }
      .bui-grid-cell{
        border-color:$bm;
        .uploadifive-button{
          &:hover{
            text-decoration: underline;
          }
          color:$ac1;
          border:none;
          background: none;
        }
        .ui-hint{
          background:none;
          &:hover{
            color:$ac1;
            .ui-icon-close{
              background:url('../../img/icon/icon_close_3.png');
            }
            a{
              text-decoration: underline;
            }
          }
          a{
            display:inline-block;
            text-decoration: none;
            width:100px;
            vertical-align middle;
            overflow hidden;
            text-overflow ellipsis;
            white-space nowrap;
          }
          .ui-icon-close{
            &:hover{
              color:$ac1;
            }
            margin-left:0;
            margin-right:5px;
          }
        }
      }
      .bui-grid-hd-inner{
        background:$bg1;
      }
      .bui-grid-hd-inner,.bui-grid-cell-inner{
        text-align:center;
        height:28px;
        line-height:26px;
        padding-top:0;
        padding-bottom:0;
      }
      .bui-grid-cell-text,.bui-grid-hd-title{
        text-overflow ellipsis;
        white-space:nowrap;
        overflow:hidden;
        max-width:80%;
        display:inline-block;
        vertical-align middle;
      }
      .bui-grid-cell-text{
        .ui-form-radio{
          .ui-form{
            >div{
              setMar0();
              display:inline-block;
              margin:0 3px;
            }
          }
        }
      }

      //表头排序图标
      .sort-asc{
        .bui-grid-sort-icon{
          border-bottom:6px solid $fc1;
          border-left:5px solid transparent;
          border-right:5px solid transparent;
          border-top:none;
        }
      }
      .bui-grid-sort-icon{
        width:0;
        height:0;
        border-top:6px solid $fc1;
        border-left:5px solid transparent;
        border-right:5px solid transparent;
        vertical-align: middle;
        margin-left:5px;
      }
      .sort-desc{
        .bui-grid-sort-icon{
          border-top:6px solid $fc1;
          border-left:5px solid transparent;
          border-right:5px solid transparent;
          border-bottom:none;
        }
      }
      .bui-grid-hd-menu-trigger{
        background:$bg4;
        &:after{
          content:' ';
          display inline-block;
          border-top:4px solid $fc3;
          border-left:4px solid transparent;
          border-right:4px solid transparent;
          border-bottom:none;
        }
      }
    }

    //-----表头-----//
    .bui-grid-header-container{
      .bui-grid-header{
        width:100% !important;
      }
    //表头全选
      .bui-grid-hd{
        .bui-grid-checkBox-container{
          &:after{
            addWC('全选');
            color:$mc;
            margin-left:5px;
          }
        }
      }
    }

    //表身
    .bui-grid-body{
      width:100% !important;
      overflow-y:initial;
      overflow-x:auto;

      .x-grid-rownumber{
        background:$fc3;
      }

      .bui-grid-checkBox-container{
        span{
          margin-right:28px;
        }
      }
      //操作块
      .grid-command{
        cursor pointer;
        setPad0()
        &:last-child{
          border:none;
        }
        border-right:1px solid $bm;
        padding-right:10px;
        padding-left:10px;
        margin-right:0;
        margin-bottom: 5px;
        line-height: 1;
        .ui-icon{
          margin-right:10px;
        }
      }

        //输入框错误
        //
      .not-found{
        text-align: center;
        padding: 20px;
        background:$bgb;
        color:$ac1;
      }

        //背景绿 红
      .green.bui-grid-row{
        .bui-grid-cell{
          &:nth-last-child(2){
            background:$bgb;
          }
        }
          background:$ac3;
        .bui-grid-cell-text{
          color:$fc3;
        }
      }
      .red.bui-grid-row{
        .bui-grid-cell{
          &:nth-last-child(2){
            background:$bgb;
          }
        }
        background:$ac1;
        .bui-grid-cell-text{
          color:$fc3;
        }
      }
      .yellow.bui-grid-row{
        .bui-grid-cell{
          &:nth-last-child(2){
            background:$bgb;
          }
        }
        background:$ac2;
        .bui-grid-cell-text{
          color:$fc3;
        }
      }
    }

    .forbidden.bui-grid-body{
      background:$bg3;
    }

      //加减号
    .bui-grid-cascade{
      &:before{
        content:"+";
        font-size:30px;
      }
      .bui-grid-cascade-icon{
        display:none;
      }
    }
    .bui-grid-cascade.bui-grid-cascade-expand{
      &:before{
        content:"—";
        font-size:18px;
        font-weight:bold;
        color:$ac1;
      }
    }

  //字颜色红绿
    .textgreen{
      color:$ac !important;
    }

    .textred{
      color:$ac1 !important;
    }

    .forbiddenttext{
      color:$fc1 !important;
    }

    .forbiddentbg{
      background:$fc1 !important;
    }

    .warningbg{
      background :#ffd3d3 !important;
    }

    .greenbg{
      background :#C4EAC4 !important;
    }

  }

