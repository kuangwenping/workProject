  //-----------进度条------------//
  .ui-strip-wrap{
    background:$bg1;
    height:8px;
    border-radius: 4px ;
    text-align: left;
    display:inline-block;
    position:relative;
    >.ui-strip{
      height:8px;
      border-radius: 4px ;
      position: absolute;
      left: 0;
      background: $ac;
    }
  }


  //-----------日历------------//
  .take-time{
    position:relative;
    >i{
      position:absolute;
      top:8px;
      right:118px;
    }
    .calendar{
      padding-left:30px;
    }
  }
  .x-monthpicker-yearnav{
    .x-monthpicker-yearnav-next,.x-monthpicker-yearnav-prev{
      border:none;
      background:none;
      span{
        display:inline-block;
        border-top:6px solid transparent;
        border-bottom:6px solid transparent;
