  @import "./var";
  @import "./mixin";
  @import "./icon";
  @import "common-module"

  body, div, span, header, footer, nav, section, aside, article, ul, dl, dt, dd, li, a, p, h1, h2, h3, h4, h5, h6, i, b, textarea, button, input, select, figure, figcaption,form {
    padding: 0;
    margin: 0;
    list-style: none;
    font-style: normal;
    text-decoration: none;
    border: none;
    box-sizing: border-box;
    font-family: "Microsoft Yahei", sans-serif;
    -webkit-tap-highlight-color: transparent;
    -webkit-font-smoothing: antialiased;
    &:focus {
      outline: none;
    }
  }

  body{
    background:$bg3;
  }

  img {
    vertical-align: middle;
  }

  a:hover, a:focus{
    text-decoration:none;
  }

  a{
    color:$mc;
  }

  iframe>body{
    overflow hidden;
  }
  .clear:after {
    content: '';
    display: block;
    clear: both;
  }

  .clear {
    zoom: 1;
  }
  .margin-b{
    margin-bottom:10px;
  }
  .margin-r{
    margin-right:10px;
  }
  .margin-t{
    margin-top:10px;
  }
  .margin-l0{
    margin-left:0;
  }
  .margin-l10{
    margin-left:10px;
  }
  .margin-t3{
    margin-top:30px;
  }

  .ellipsis {
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
  }

  //最大宽
  .ui-max-width{
    max-width:1440px;
    margin:0 auto;
  }

  //字号
  .ui-f14{
    font-size:14px;
  }

  .ui-f12{
    font-size:12px;
  }

    //颜色
  .ui-fac{
    color:$ac;
  }

  .ui-f-Lucida{
    font-family: Lucida Sans Typewriter;
  }

  //-----------------------布局切断分割------------------------//
  //侧面
  .aside-panel{
    position:fixed;
    width:280px;
    bottom:0;
    top:0;
    background:$bg8;
  }

  //内容块
  .content{
    margin-left: 280px;
    padding:18px 0;
  }

  .content-wrap{
    padding: 0 20px;
  }

  //-----------------------ui点击分割------------------------//
  //菜单上传按钮
  .uploadifive-button{
    display: inline-block;
    background: #fff;
    color: #3ba2b9;
    border: 1px solid #3ba2b9;
    border-radius: 4px;
    vertical-align: middle;
    cursor: pointer;
  }
  .uploadifive-queue {
    display: inline-block;
  }

  //按钮
  .ui-btn[disabled="disabled"]{
    &:hover{
      background:$bg2;
      color:$fc3;
      cursor:default;
    }
    background:$bg2;
    color:$fc3;
    opacity :0.5;
  }

  .ui-btn{
    height:28px;
    line-height:26px;
    text-align: center;
    border-radius: 4px;
    font-size:12px;
    display:inline-block;
    vertical-align middle;
    cursor pointer;
  }

  .ui-btn-wide{
    width:100px;
  }

  .ui-btn-narrow{
    width:80px;
  }


  .ui-btn-full{
    &:hover{
      background: $bg5;
    }
    background: $bg4;
    color: $fc3;
  }

  .ui-btn-empty{
    &:hover{
      background:$bg5;
      color:$fc3;
    }
    background:$fc3;
    color:$bg4;
    border:1px solid $bg4;
  }

  .ui-btn-wring{
    &:hover{
      background:#d93c36;
    }
    background: $ac1;
    color:$fc3;
  }

  .ui-btn-search{
    &:hover{
      background: #ffe792;
    }
    background: $ac2;
    font-size:14px;
    color:$mc;
  }

  .ui-btn-stop{
    background:$bg2;
    color:$fc3;
  }

  .ui-btn-go{
    &:hover{
      opacity :.9;
    }
    color:$fc3;
    background:$ac3;
  }

  //提示
  .ui-hint{
    height:24px;
    line-height:24px;
    padding-left:10px;
    padding-right:10px;
    background:$bg1;
    color:$mc;
    display:inline-block;
    vertical-align middle;
    cursor pointer;s
    .ui-icon-close{
      sbg('../../img/icon/icon_close_3.png');
      width:10px;
      height:10px;
      margin-bottom:3px;
      margin-left:10px;
      cursor :pointer;
    }
    &:hover{
      background:$bg4;
      color:$fc3;
      .ui-icon-close{
        sbg('../../img/icon/icon_close_4.png');
        width:10px;
        height:10px;
      }
    }
  }
  .active.ui-hint{
    background:$bg4;
    color:$fc3;
    .ui-icon-close{
      sbg('../../img/icon/icon_close_4.png');
      width:10px;
      height:10px;
    }
  }


  //表单
  //文本输入框
  input.ui-input{
    border:1px solid $bm;
    height:28px;
    border-radius: 0;
    padding-left:10px;
    font-size:12px;
  }

  input.ui-input-clone{
    @extend input.ui-input;
  }

  .form-item{
    display:inline-block;
    serMar0();
    margin:0 10px;
    vertical-align middle;
    .ui-text-form{
      vertical-align middle;
    }
  }

  //单选
  .ui-radio-as{
    width:16px;
    height:16px;
    border:1px solid $bc;
    background:$bg8;
    border-radius 50%;
    display:inline-block;
    vertical-align middle;
    position:relative;
    &:after{
      position:absolute;
      addWC(' ');
      width:12px;
      height:12px;
      background:$bg8;
      border-radius 50%;
      top:2px;
      left:2px;
    }
  }

  .ui-radio{
    @extend .ui-radio-as
    width:18px;
    height:18px;
    border:1px solid $bm;
    background:$bgb;
    margin-right:5px;
    &:after{
      background:$bgb;
      top:3px;
      left:3px;
    }
  }
  input[type="radio"]{
     display:none;
  }
  input[type="radio"]:checked + .ui-radio-as{
    &:after{
      background:$bg4;
    }
  }
  input[type="radio"]:checked + .ui-radio{
    &:after{
      background:$bg4;
    }
  }

  input[readonly="readonly"] + .ui-radio{
    background:bg1 !important;
  }

  input[readonly="readonly"]:checked + .ui-radio{
    &:after{
      background:none;
    }
  }

  input[readonly="readonly"] + .ui-radio-as{
    background:$bg1 !important;
  }
  input[readonly="readonly"]:checked + .ui-radio-as{
    &:after{
      background:none;
    }
  }
  .ui-radio.select{
    position:relative;
    &:after{
      addWC(' ');
      position:absolute;
      width:12px;
      height:12px;
      background:$bg4;
      border-radius 50%;
      top:3px;
      left:3px;
    }
  }
  .ui-radio-as.select{
    position:relative;
    &:after{
      position:absolute;
      addWC(' ');
      width:12px;
      height:12px;
      background:$bg4;
      border-radius 50%;
      top:2px;
      left:2px;
    }
  }


