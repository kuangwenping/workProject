@import "../../assets/css/common/var.styl";
@import "../../assets/css/common/mixin.styl";

.dataout-wrap{
  max-width:1200px;
  margin:0 auto;
  padding:40px;
}
  .out-thunk{
    margin-bottom:60px;
    position:relative;
    .ui-key-start{
      position:absolute;
      top:50%;
      transform :translateY(-50%);
      width:75px;
      text-align :right;
      margin-right :30px;
      font-size:16px;
    }
    .out-list{
      margin-left:105px;
      li{
        setMar0();
        float:left;
        margin-right:10px;
        width:248px;
        height:140px;
        text-align :center;
        padding:40px 0 25px;
        border:1px solid transparent;
        .btn-text{
          margin-bottom:20px;
          .ips-ui-icon{
            margin-right:8px;
          }
        }
      }
      .ui-green{
        border-color:$ac3;
        .ui-btn{
          background:$ac3;
        }
        .btn-text{
          color:$ac3;
        }
      }
      .ui-grey{
        border-color:$bg2;
        .ui-btn{
          background:$bg2;
        }
        .btn-text{
          color:$bg2;
        }
      }
      .ui-red{
        border-color:$ac1;
        .ui-btn{
          background:$ac1;
        }
        .btn-text{
          color:$ac1;
        }
      }
    }
  }